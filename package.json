{
  "scripts": {
    "start": "webpack-dev-server --hot --inline --progress --history-api-fallback --host 0.0.0.0 --port 5000",
    "build": "rm -rf dist && NODE_ENV=production webpack --config webpack.config.build.js && cp -r static/* dist && cat static/_redirects | sed \"s|__RESOURCE_DOCS__|${RESOURCE_DOCS}|g\" > dist/_redirects",
    "deploy": "npm run build && netlify deploy -p dist -s $NETLIFY_ID -t $NETLIFY_TOKEN",
    "test": "tslint \"src/**/*.ts{,x}\""
  },
